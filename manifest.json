{
  "manifest_version": 2,
  "name": "dissolveChromeExtension",
  "description": "A chrome extension for dissolve's personal use. 100% useless.",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "favicon.png",
    "default_title": "dissolveChromeExtension",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["index.js"],
    "persistent": false
  }
}